{
  "$schema": "vscode://schemas/settings/user",
  "breadcrumbs.enabled": false,
  "codesnap.shutterAction": "copy",
  "cSpell.language": "en,pt,pt_BR",
  "cSpell.userWords": ["Dunkel", "esbenp", "gitblame", "Menlo"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Ubuntu Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.lineHeight": 1.618,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "boundary",
  "errorLens.enabledDiagnosticLevels": ["error", "warning"],
  "explorer.compactFolders": false,
  "explorer.sortOrder": "type",
  "gitblame.inlineMessageEnabled": true,
  "gitblame.inlineMessageFormat": "${author.name} (${time.ago}) [${commit.hash_short}]",
  "security.workspace.trust.untrustedFiles": "open",
  "supermaven.enable": {
    "*": true
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "UbuntuMono Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 15,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": false,
  "workbench.tree.indent": 16,
  "zenMode.hideLineNumbers": false,
  "nix.enableLanguageServer": true,
  "nix.serverPath": "nixd",
  "workbench.iconTheme": "symbols",
  "symbols.hidesExplorerArrows": false
}
